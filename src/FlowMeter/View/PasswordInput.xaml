<Window x:Class="FlowMeter.View.PasswordInput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlowMeter.View"
        mc:Ignorable="d"
        Title="PasswordInput" Height="209.005" Width="289.218" WindowStartupLocation="CenterOwner">
    <Grid Loaded="Grid_Loaded">
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnOk" Content="Ok" HorizontalAlignment="Left" Margin="45,120,0,0" VerticalAlignment="Top" Width="75" Height="Auto" IsDefault="True" Click="BtnOk_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="161,120,0,0" VerticalAlignment="Top" Width="75" Height="Auto" IsCancel="True" Click="BtnCancel_Click"/>
        <Label Content="Password :" HorizontalAlignment="Left" Margin="29,51,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="pwdExternal" HorizontalAlignment="Left" Margin="95,52,0,0" VerticalAlignment="Top" Width="141" TabIndex="0"/>

    </Grid>
</Window>
